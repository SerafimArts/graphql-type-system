{
    "name": "railt/type-system",
    "description": "Interoperable GraphQL Type System implementation",
    "license": "MIT",
    "keywords": [
        "graphql",
        "language",
        "sdl",
        "type system",
        "interop",
        "abstraction"
    ],
    "homepage": "https://github.com/railt",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/type-system"
    },
    "require": {
        "php": ">=7.4",
        "phplrt/lexer": "^2.3",
        "railt/common": "^2.0",
        "serafim/immutable": "^1.0",
        "graphql/type-system-contracts": "^14.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\TypeSystem\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\TypeSystem\\Tests\\": "tests"
        }
    },
    "provide": {
        "graphql/type-system-implementation": "~14.0"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
